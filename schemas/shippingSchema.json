{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://localhost:3001/ship/shipping.schema.json",
    "title": "Order",
    "description": "Shipping order",
    "type": "object",
    "properties": {
        "productId": {
            "type": "integer",
            "minimum": 1000
        },
        "name": {"type": "string"},
        "addr": {"type": "string"},
        "zip": {"zip": "string"}
    },
    "additionalProperties": false,
    "required": [
        "productId",
        "name",
        "addr",
        "zip"
    ]
}